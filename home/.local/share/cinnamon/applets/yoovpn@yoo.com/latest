<style>
  #designstudio-button {
    border-radius: 0.4rem 0.4rem 0 0;
    bottom: 0 !important;
    overflow: hidden;
    right: 1rem !important;
  }
</style>
<script>
  var trigger = document.getElementsByClassName('snapengage-trigger');
  function loadLiveChat() {
    var done = false;
    var livechatElem = document.getElementById('livechat_container');
    var parentScript = document.getElementsByTagName('script')[0];
    var snapEngageScript = document.createElement('script');
    function openLiveChat() {
      if (!done && (!this.readyState || this.readyState === 'loaded' || this.readyState === 'complete')) {
        done = true;
        SnapEngage.getAgentStatusAsync(function (online) {
          if (online) {
            $('.contact-us-methods').show();
            $('.contact-us-method-chat').show();
          } else {
            $('.contact-us-methods').show();
            $('.contact-us-method-chat').hide();
            $('.contact-us-method-ticket').addClass('col-sm-offset-2');
          }
          $('.contact-us-loading').hide();
        });
  
        SnapEngage.setCallback('Open', function () {
          iframe = document.getElementById('designstudio-iframe');
          iframe.contentWindow.document.getElementById('message-textarea').style.outline = '2px solid #757575';
        });
      }
    }
    snapEngageScript.type = 'text/javascript';
    snapEngageScript.src = "https://storage.googleapis.com/code.snapengage.com/js/5d60707d-4dae-4629-97cd-39cfa1abbb6d.js";
    snapEngageScript.defer = true;
    snapEngageScript.onload = openLiveChat;
    snapEngageScript.onreadystatechange = openLiveChat;
    parentScript.parentNode.insertBefore(snapEngageScript, parentScript);
  }
  
  loadLiveChat();
  
  if (window.location.hash === '#open-chat') loadLiveChat();
  
  function bindClick(elem) {
    elem.addEventListener('click', function (event) {
      if (typeof SnapEngage === 'undefined') {
        loadLiveChat();
      } else {
        event.preventDefault();
        SnapEngage.startLink();
      }
    });
  }
  if (trigger.length > 0) {
    for (var i = 0; i < trigger.length; i += 1) {
      bindClick(trigger[i]);
    }
  }
</script>



